<interface>
  <object id="adjustScale" class="GtkAdjustment">
     <property name="lower">0</property>
     <property name="upper">360</property>
  </object>
  <object id="window" class="GtkWindow">
    <property name="visible">True</property>
    <property name="title">AutoControl</property>
    <property name="border-width">25</property>
    <child>
      <object id="grid" class="GtkGrid">
        <property name="visible">True</property>
        <child>
          <object id="ignitionLabel" class="GtkLabel">
            <property name="visible">True</property>
	        <property name="label">Ignition: </property>
            <property name="hexpand">True</property>
            <property name="vexpand">True</property>
          </object>
          <packing>
            <property name="left-attach">0</property>
            <property name="top-attach">0</property>
          </packing>
        </child>
        <child>
          <object id="ignitionSwitch" class="GtkSwitch">
            <property name="visible">True</property>
            <property name="hexpand">True</property>
            <property name="vexpand">True</property>
          </object>
          <packing>
            <property name="left-attach">1</property>
            <property name="top-attach">0</property>
          </packing>
        </child>
        <child>
          <object id="angleSlider" class="GtkScale">
            <property name="visible">True</property>
            <property name="hexpand">True</property>
            <property name="vexpand">True</property>
            <property name="inverted">False</property>
            <property name="adjustment">adjustScale</property>
          </object>
          <packing>
            <property name="left-attach">1</property>
            <property name="top-attach">1</property>
          </packing>
        </child>
      </object>
      <packing>
      </packing>
    </child>
  </object>
</interface>
